<view class="zzx-tabs"><view class="zzx-tabs-wrap"><view data-event-opts="{{[['tap',[['_onClick',[index]]]]]}}" class="zzx-tab" style="{{'width:'+(iwidth)+';'+('color:'+(index===currentIndex?activeColor:defaultColor)+';')}}" onTap="__e" a:for="{{items}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{$slots.default}}"><slot item="{{item}}"></slot></block><block a:else><text class="{{(('item-title')+' '+(dots[index]?'dot-show':''))}}">{{item}}</text></block></view></view><view class="zzx-tabs-bar" style="{{'width:'+(iwidth)+';'+('transform:'+(moveDistance)+';')+('margin-top:'+(lineTop)+';')}}"><view class="zzx-bottom-line" style="{{'height:'+(lineHeight)+';'+('width:'+(lineWidth)+';')+('background:'+(activeColor)+';')}}"></view></view></view>