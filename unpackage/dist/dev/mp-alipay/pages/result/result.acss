
.price{
	background: url(/static/bg.png) no-repeat;
	text-align: center;
	padding: 100rpx 0;
	background-size: 100%;
	height: 220rpx;
	color: #ab5631;
	font-size: 36rpx;
	font-weight: bold;
	position: relative;
}
.price-info{
	color: #fff;
	font-size: 60rpx;
	font-weight: bold;
	margin: 30rpx 0;
}
.good-props{
	background: rgba(205,185,124,0.2);
	border-radius:12px;
	padding: 30rpx;
	margin: 30rpx auto;
	width: 600rpx;
	color: #856a4d;
	display: block;
	position: relative;
	left: 0;
	top: -90rpx;
	right: 0;
	z-index: 1;
}
.good-props-list{
	line-height: 80rpx;
}
.remind{
	margin: 30rpx;
	padding: 30rpx;
	font-size: 28rpx;
	color: #999;
	line-height: 70rpx;
}
.sell{
	background-color: #fed22b;
	height:80rpx;
	line-height: 80rpx;
	width: 500rpx;
	margin: 0 auto 60rpx;
	border-radius: 4px;
	text-align: center;
	color: #865610;
}



