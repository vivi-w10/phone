
.navbar-position{
	position: fixed;
	top: 0;
	left: 0;
	z-index: 99;
	width: 100%;
}
.navbar{
	height: 110rpx;
	border-bottom: 2px solid #f3f3f3;
	padding: 0 20rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	margin: 0;
	background: #fff;
	box-sizing: border-box;
}
.navbar-logo{
	width: 200rpx;
	-webkit-flex-shrink: 0;
	        flex-shrink: 0;
}
.navbar-search{
	background: #f3f3f3;
	border-radius: 16rpx;
	width: 440rpx;
	padding: 8rpx 20rpx;
	margin-left: 20rpx;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.navber-search-txt{
	color: #999;
	font-size: 24rpx;
	margin-left: 10rpx;
	background-color: #f3f3f3;
}

